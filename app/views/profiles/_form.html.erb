<ul>
<% @profile.errors.full_messages.each do |message| %>
    <li> <%= message %> </li>
<% end %>
</ul>

<%= form_with model: @profile, local:true do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>
  <div>
  <%= f.label :telephone %>
  <%= f.text_field :telephone %>
  </div>
  <%= f.submit 'Salvar dados'%>
<%end%>
